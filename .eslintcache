rience, \r\n                types:           tmpTypes,\r\n                descriptions:    extra.flavor_text_entries,\r\n                names:           tmpNames\r\n            });\r\n\r\n        }catch(e){\r\n            console.error(e);\r\n        }\r\n        setLoading(false);\r\n\r\n    }\r\n\r\n    const _pkmData = () => {\r\n        if(extraData)\r\n        {\r\n            return(\r\n                <PokedexExtraData data={extraData}/>\r\n            \r\n            )\r\n        }\r\n    }\r\n\r\n    const _renderPkm = () => {\r\n        if(pkmList){\r\n            return(\r\n                <>\r\n                <div className=\"pkm-cards\">\r\n                    {pkmList.map((pkm) => <PkmItem key={pkm.id} data={pkm} action={setSelectedPkm} selectedPkm={selectedPkm} extraData={() => _pkmData()} />)}\r\n                </div>\r\n                {!selectedPkm && !loading && <div className=\"dex-container\">\r\n                    <button className=\"cta-button cta-button--green\" type=\"button\" onClick={() => setLimit(limit + 6)}>More</button>\r\n                </div>}\r\n                </>\r\n            )\r\n        }else{\r\n            if(!loading){\r\n                return(\r\n                    <div className=\"dex-container\">\r\n                        <p>No result...</p>\r\n                    </div>\r\n                )\r\n            }\r\n\r\n        }\r\n    }\r\n\r\n    const _changeOffset = (value) => {\r\n        value = parseInt(value, 10);\r\n        if(isNaN(value) || value > MAX_PKM_NB || value < 0) { \r\n            return null; \r\n        }\r\n        setOffSet(value);\r\n    }\r\n\r\n    useEffect( () => {\r\n        if(selectedPkm){\r\n            setExtraData(null);\r\n            fetchPkmData();\r\n        }\r\n    }, [selectedPkm]);\r\n\r\n    useEffect(()=> {\r\n        fetchPkm();\r\n    }, [offSet, limit]);\r\n\r\n    return(\r\n        <div className=\"pokedex-div\">\r\n            <div className=\"container\">\r\n      